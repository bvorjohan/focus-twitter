{
    "name": "Focus Twitter",
    "description": "Twitter without the distractions",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": [],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "js": ["content.js"]
      }
    ]
  }